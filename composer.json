{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./packages/teksite/handler"
        }, {
            "type": "path",
            "url": "./packages/teksite/extralaravel"
        }, {
            "type": "path",
            "url": "./packages/teksite/authorize"
        }, {
            "type": "path",
            "url": "./packages/teksite/module"
        }, {
            "type": "path",
            "url": "./packages/teksite/lareon"
        }
    ],
    "require": {
        "php": "^8.2",
        "alexusmai/laravel-file-manager": "^3.3",
        "intervention/image": "^3.11",
        "laravel/fortify": "^1.25",
        "laravel/framework": "^12.0",
        "laravel/socialite": "^5.20",
        "laravel/tinker": "^2.10.1",
        "maatwebsite/excel": "^3.1",
        "morilog/jalali": "*",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/schema-org": "^3.23",
        "staudenmeir/laravel-adjacency-list": "^1.24",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",

            "Teksite\\Handler\\": "packages/teksite/handler/src/",
            "Teksite\\Extralaravel\\": "packages/teksite/extralaravel/src/",
            "Teksite\\Authorize\\": "packages/teksite/authorize/src/",
            "Teksite\\Module\\": "packages/teksite/module/src/",
            "Teksite\\Lareon\\": "packages/teksite/lareon/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "packages/teksite/extralaravel/composer.json",
                "packages/teksite/handler/composer.json",
                "packages/teksite/authorize/composer.json",
                "packages/teksite/module/composer.json",
                "packages/teksite/lareon/composer.json",
                "Lareon/Modules/*/composer.json",
                "Lareon/CMS/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": true
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
